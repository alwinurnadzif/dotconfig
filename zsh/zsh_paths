export PATH="$HOME/.local/bin:$PATH"

export WORKSPACE_APPS="$HOME/workspace/apps"
# golang

export PATH="$WORKSPACE_APPS/go/bin:$PATH"
export GOPATH="$HOME/go"
export PATH="$GOPATH/bin:$PATH"
# node js
export PATH="$WORKSPACE_APPS/node/bin:$PATH"
# dart sdk 
export PATH="$WORKSPACE_APPS/dart-sdk/bin:$PATH"
# Xampp  
export PATH="/opt/lampp/bin:$PATH"

export PATH="/opt/nvim/bin:$PATH"
export PATH="$HOME/.local/kitty.app/bin:$PATH"

