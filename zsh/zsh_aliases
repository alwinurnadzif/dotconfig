# Text Editor
alias v="nvim"

# Git 
alias ga="git add"
alias gc="git commit"
alias gcmsg="git commit -m"
alias gb="git branch"
alias gco="git checkout"
alias gss="git status -s"
alias gp="git push"
alias glog="git log --oneline --decorate --graph"
alias gd="git diff"
alias gdclip="git diff | xclip -sel clipboard"


# xampp 
alias xampp="sudo /opt/lampp/lampp start"


